{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://backendshop-production-963a.up.railway.app/api/"
      }
    ],
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "/src/App.tsx",
        "statusCode": 200
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          }
        ]
      }
    ]
  }
  