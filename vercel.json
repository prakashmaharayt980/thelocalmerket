{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://backendshop-production-963a.up.railway.app/api/"
      }
    ],
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "/index.html",
        "statusCode": 200
      }
    ]
  }
  